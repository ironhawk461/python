import random
a = int(input("Enter the beginning number: "))
b = int(input("Enter the ending number: "))
c = random.randrange(a, b + 1)
d = int(input("Now enter your guess: "))
while b + 2 !=d:
    if d != c and d > c:
        print("incorrect your guess is too high")
        d = int(input("try again: "))
    elif d != c and d < c:
        print("incorrect your guess is too low")
        d = int(input("try again: "))
    else:
        c = str(c)
        print("Your guess is correct the number is " + c)
        break
